    "def get_destinations(self, passengers):\n",
    "    destinations = {\n",
    "        'waiting': [],\n",
    "        'in elevator': []\n",
    "    }\n",
    "    for passenger in passengers:\n",
    "        destinations['waiting'].append((passenger.start_floor, passenger.end_floor))\n",
    "        \n",
    "    return destinations"
   ]
